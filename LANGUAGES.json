[
  {
    "language": "Rust",
    "extensions": [
      ".rs"
    ],
    "markers": [
      {
        "type": "line",
        "pattern": "//"
      },
        {
        "type": "block",
        "pattern": {
          "start": "/\\*",
          "end": "\\*/"
        }
      },
      {
        "type": "line",
        "pattern": "///"
      },
        {
            "type": "line",
            "pattern": "//!"
        },
        {
            "type": "block",
            "pattern": {
            "start": "/\\*!",
            "end": "\\*/"
            }
        },
        {
            "type": "block",
            "pattern": {
                "start": "todo!(",
                "end": ")"
            }
        }
    ]
  }
]
