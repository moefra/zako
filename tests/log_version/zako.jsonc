{
    "group" : "com.example",
    "artifact" : "benchmark",
    "version" : "1.0.0",
    // builds = ["./"] # Root `BUILD.ts`
    "dependencies": {
        "guava-fork" : "fra.moe:guava",
        "guava" : {
                "package" : "com.google:guava",
                "version" : "2.0.0",
                "optional" : true
            }
    },
    "config":{
        "log" : {
            "type" : "boolean",
            "default" : true,
            "description" : "Enable logging"
        },
        "debug" : {
            "type" : "boolean",
            "description" : "Enable debug logging",
            "inherit" : "global//config/debug"
        },
        "guava-cpp20" : {
            "type" : "boolean",
            "description" : "Enable C++20 features from guava",
            "inherit" : "guava//config/cpp20"
        }
    }
}
