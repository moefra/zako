{
    "compilerOptions": {
        "paths": {
            "zako:semver": ["../../../dist/semver/index.d.ts"],
            "zako:rt": ["../../../dist/rt.d.mts"],
            "zako:syscall": ["../../../dist/syscall.d.ts"],
            "zako:core": ["../../dist/core.d.mts"]
        },
        "moduleResolution": "bundler",
        "allowJs": false,
        "checkJs": false,
        "strict": true,
        "allowImportingTsExtensions": false,
        "alwaysStrict": true,
        "target": "ESNext",
        "module": "ESNext",
        "newLine": "lf",
        "verbatimModuleSyntax": true,
        "erasableSyntaxOnly": true,
        "isolatedModules": true,
        "isolatedDeclarations": true,
        "declaration": true,
        "inlineSourceMap": false,
        "inlineSources": false,
        "esModuleInterop": false
    },
    "files": ["./rt.ts", "./core.ts"]
}
